{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "include": ["src", "types", "services", "controllers", "routes"]
}
